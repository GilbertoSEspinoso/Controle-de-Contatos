@model ContactModel
@{
    ViewData["Title"] = "Listagem de contatos";
}
<div id="area-principal">

    <div class="centralizarTexto">
        <h3>Deseja realmente apagar o contato (@Model.Name) ?</h3>
    </div>

    <div class="postagem">
        <a class="btn btn-success  btn-block" role="button" asp-controller="Contact" asp-action="Index">Cancelar</a>
        <a class="btn btn-secondary btn-danger btn-block" asp-controller="Contact" asp-action="Delete" asp-route-id="@Model.Id" role="button">Confirmar</a>
    </div>
</div>



