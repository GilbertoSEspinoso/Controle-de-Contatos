@model ContactModel
@{
    ViewData["Title"] = "Listagem de contatos";
}
<div class="text-center">

    <h3>Deseja realmente apagar o contato (@Model.Name) ?</h3>
</div>

<a class="btn btn-primary  btn-block" role="button" asp-controller="Contact" asp-action="Index">Cancelar</a>
<a class="btn btn-secondary btn-danger btn-block" role="button" asp-controller="Contact" asp-action="Delete" asp-route-id="@Model.Id">Confirmar</a>



